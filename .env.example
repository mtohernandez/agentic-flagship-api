# Required — Groq API key for LLM inference
GROQ_API_KEY=gsk_your_key_here

# Groq model configuration
GROQ_MODEL=llama-3.3-70b-versatile
GROQ_TEMPERATURE=0.0

# Agent limits
AGENT_RECURSION_LIMIT=40
AGENT_REQUEST_TIMEOUT=300
AGENT_MAX_RETRIES=2

# Groq max tokens (higher = fewer truncated tool-call JSON errors)
GROQ_MAX_TOKENS=2048

# Browser (set to false on serverless platforms like Vercel to skip Playwright)
BROWSER_ENABLED=true
BROWSER_HEADLESS=true

# CORS — comma-separated list of allowed origins
CORS_ORIGINS=*

# Required — comma-separated list of valid API keys
API_KEYS=your-api-key-1,your-api-key-2

# Rate limiting — requests per minute per API key
RATE_LIMIT_RPM=20

# Debug mode
DEBUG=false
